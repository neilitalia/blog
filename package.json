{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "(trap 'kill 0' INT EXIT; npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/index.css --watch & gatsby develop)",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "tailwind-watch": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/index.css --watch"
  },
  "dependencies": {
    "@fontsource/jost": "^4.5.1",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "autoprefixer": "^10.4.0",
    "gatsby": "^4.4.0",
    "gatsby-plugin-mdx": "^3.4.0",
    "gatsby-plugin-postcss": "^5.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "postcss": "^8.4.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^3.0.7"
  },
  "devDependencies": {
    "prettier": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
